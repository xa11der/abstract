{
  "WorkflowID": 1,
  "WorkflowName": "Allow only specific IP for ADMIN role",
  "Path": "/admin/*",
  "Params": [
    {
      "Name": "ip_address",
      "Expression": "$request.getIpAddress"
    },
    {
      "Name": "user_role",
      "Expression": "$user.getRole"
    }
  ],
  "Rules": [
    {
      "RuleName": "Allow only specific IP",
      "Expression": "$ip_address == '100.100.100.100'"
    },
    {
      "RuleName": "Check role",
      "Expression": "$user_role == 'ADMIN'"
    }
  ]
}